{
  "private": true,
  "packageManager": "pnpm@6.23.6",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@puik/components": "workspace:*",
    "@puik/theme": "workspace:*",
    "@puik/utils": "workspace:*",
    "@vueuse/core": "^7.5.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@pnpm/find-workspace-packages": "^3.1.34",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^7.8.0",
    "@sucrase/jest-plugin": "2.2.0",
    "@types/gulp": "^4.0.9",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@vue/runtime-core": "^3.2.26",
    "@vue/shared": "^3.2.26",
    "chalk": "^5.0.0",
    "csstype": "2.6.19",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.14.10",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.2.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.2.0",
    "expect-type": "0.13.0",
    "gulp": "^4.0.2",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.1.5",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "rimraf": "^3.0.2",
    "sass": "1.45.2",
    "sucrase": "^3.20.3",
    "ts-jest": "^27.1.2",
    "ts-morph": "^13.0.2",
    "typescript": "^4.5.4",
    "vue": "^3.2.26",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "^0.30.2"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "engines": {
    "node": ">= 16"
  },
  "version": "0.0.1",
  "description": "prestashop ui kit with tailwind",
  "scripts": {
    "cz": "git-cz",
    "build:theme": "pnpm run -C packages/theme build",
    "docs:dev": "pnpm run -C docs dev",
    "docs:serve": "pnpm run -C docs serve",
    "docs:build": "pnpm run -C docs build",
    "format": "prettier --write .",
    "gen:version": "sucrase-node scripts/gen-version.ts",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx && pretty-quick --branch dev",
    "prepare": "husky install",
    "postinstall": "pnpm gen:version",
    "preinstall": "npx only-allow pnpm -y",
    "storybook": "pnpm run -C docs storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrestaShopCorp/puik.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PrestaShopCorp/puik/issues"
  },
  "homepage": "https://github.com/PrestaShopCorp/puik#readme"
}
